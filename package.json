{
  "name": "cocoa-web-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.3",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "json-server": "^0.15.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "json-server -w db.json -p 3001",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint -h tsconfig.json --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.8",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-virtualized": "^9.21.2",
    "@types/recharts": "^1.1.19",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga": "^0.10.5",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.14",
    "express": "^4.17.1",
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "react-virtualized": "^9.21.1",
    "recharts": "^1.6.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.3",
    "typesafe-actions": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100
  },
  "homepage": "./"
}
